{
  "name": "coini-app-nestjs-service-boilerplate",
  "version": "1.0.0",
  "description": "Coini App NestJS Service Boilerplate",
  "author": "Coini App",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "tsnd -r tsconfig-paths/register --inspect=0.0.0.0:9229 --respawn src/main.ts",
    "test": "rimraf coverage .nyc_output && concurrently 'npm:test:unit' 'npm:test:e2e' && npm run calculate-global-test-coverage",
    "test:unit": "jest tests/unit --coverageDirectory coverage/unit --detectOpenHandles",
    "test:e2e": "jest tests/e2e --coverageDirectory coverage/e2e --detectOpenHandles",
    "calculate-global-test-coverage": "ts-node scripts/calculate-global-test-coverage.ts",
    "prepare": "husky",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "build:clean": "rimraf dist; exit 0",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node ./src/database/script/migration.create.ts",
    "migration:generate": "ts-node ./src/database/script/migration.generate.ts",
    "migration:up": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:run -d ./src/database/services/database.service.ts",
    "migration:down": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:revert -d ./src/database/services/database.service.ts",
    "migration:run": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:run -d ./src/database/services/database.service.ts"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.3",
    "bcryptjs": "^2.4.3",
    "common-js": "^0.3.8",
    "dotenv": "^16.4.5",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/types": "^18.6.1",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.1",
    "@swc/jest": "^0.2.36",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nock": "^13.5.1",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
